{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "stay",
        "experience",
        "rental"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "biginteger"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured": {
      "type": "boolean"
    },
    "amenities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::amenity.amenity",
      "mappedBy": "listings"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "listings"
    },
    "provider": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::provider.provider",
      "inversedBy": "listings"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "listings"
    }
  }
}
